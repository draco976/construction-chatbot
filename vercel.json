{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm install && pip install -r server/requirements.txt",
  "devCommand": "cd frontend && npm run dev",
  "framework": null,
  "functions": {
    "server/vercel_app.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/vercel_app.py"
    }
  ]
}